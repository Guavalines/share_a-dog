<%= simple_form_for @dog do |f| %>
  <%= f.input :name %>
  <%= f.input :breed %>
  <%= f.input :born_on %>
  <%= f.input :sex %>
  <%= f.input :size%>
  <%= f.input :neutered %>
  <%= f.input :photo, as: :file %>
  <%= f.input :address %>
  <%= f.input :bio %>
  <%= f.button :submit %>
<% end %>

<%= link_to "view dogs", dogs_path, class: "btn btn-gradient" %>
